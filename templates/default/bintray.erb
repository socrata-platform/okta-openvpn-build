{
    "package": {
        "name": "<%= node['bintray']['package']['name'] %>",
        "repo": "<%= node['bintray']['package']['repo'] %>",
        "subject": "<%= node['bintray']['package']['subject'] %>",
        "desc": "<%= node['bintray']['package']['desc'] %>",
        "website_url": "<%= node['bintray']['package']['website_url'] %>",
        "issue_tracker_url": "<%= node['bintray']['package']['issue_tracker_url'] %>",
        "vcs_url": "<%= node['bintray']['package']['vcs_url'] %>",
        "github_use_tag_release_notes": false,
        "licenses": <%= node['bintray']['package']['licenses'] %>,
        "labels": <%= node['bintray']['package']['labels'] %>,
        "public_download_numbers": false
    },

    "version": {
        "name": "<%= node['bintray']['version']['name'] %>",
        "desc": "<%= node['bintray']['version']['desc'] %>",
        "released": "<%= node['bintray']['version']['released'] %>",
        "vcs_tag": "<%= node['bintray']['version']['vcs_tag'] %>",
        "gpgSign": true
    },

    "files": <%= node['bintray']['files']['distro_pattern'] %>,
    "publish": true
}
